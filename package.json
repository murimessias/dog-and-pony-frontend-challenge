{
  "name": "dog-and-pony",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "type-check:watch": "yarn type-check -w"
